;레이무
@BATTLECRY_TORIKOMODE_1, ARG, ARGS
;레이무
SELECTCASE ARG
    ;전투시작
    CASE 1
        ;여기는 패턴. 늘리고 싶은 만큼 추가해도 됨.
        SELECTCASE RAND:3
            CASE 0
                PRINTFORMW 「%ARGS%? 그런 건 식은 죽 먹기지」
            CASE 1
                PRINTFORMW 「홍백의 무녀, 등장!」
            CASE 2
                PRINTFORMW 「하쿠레이의 무녀, 등장!」
        ENDSELECT
    ;공격시
    CASE 2
        SELECTCASE RAND:3
            CASE 0
                PRINTFORMW 「받아라!」
            CASE 1
                PRINTFORMW 「이얍!」
            CASE 2
                PRINTFORMW 「으럇!」
        ENDSELECT
    ;피격시
    CASE 3
        SELECTCASE RAND:3
            CASE 0
                PRINTFORMW 「꺄악!」
            CASE 1
                PRINTFORMW 「으… %ARGS% 주제에 꽤 하잖아!」
            CASE 2
                PRINTFORMW 「아파!」
        ENDSELECT
    ;적을 죽였을 때
    CASE 4
        SELECTCASE RAND:3
            CASE 0
                PRINTFORMW 「어떠냐!」
            CASE 1
                PRINTFORMW 「요괴 버스터!」
            CASE 2
                PRINTFORMW 「아~아 이 정도 밖에 안 되다니」
        ENDSELECT
    ;빈사
    CASE 5
        SELECTCASE RAND:3
            CASE 0
                PRINTFORMW 「하아… 하아…」
            CASE 1
                PRINTFORMW 「으… 아직, 아직 싸울 수 있어!」
            CASE 2
                PRINTFORMW 「…슬슬 도망칠까나…」
        ENDSELECT
    ;쓰러질 때
    CASE 6
        SELECTCASE RAND:3
            CASE 0
                PRINTFORMW 「까야악~~!」
            CASE 1
                PRINTFORMW 「으…. 으윽…」
            CASE 2
                PRINTFORMW 「으… 안 돼…」
        ENDSELECT
    ;필살기. 보통 스펠 카드를 쓰면 좋음.
    CASE 7
        SELECTCASE RAND:5
            CASE 0
                PRINTFORMW 「신령! 몽상봉인!」
            CASE 1
                PRINTFORMW 「몽상천생!」
            CASE 2
                PRINTFORMW 「보구! 음양귀신옥!」
            CASE 3
                PRINTFORMW 「영부! 음양보옥!」
            CASE 4
                PRINTFORMW 「신기! 팔방귀박진!」
        ENDSELECT
    ;승리대사
    CASE 8
        SELECTCASE RAND:2
            CASE 0
                PRINTFORMW 「후훗. 너희들 정도로는 아직 멀었다구~!」
            CASE 1
                PRINTFORMW 「뭐, 별 것도 아니네~」
        ENDSELECT
ENDSELECT

